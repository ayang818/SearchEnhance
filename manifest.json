{
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistence": false
    },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "description": "SearchEnhance一款增强搜索能力的插件",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128":"128.png"
 },
 "manifest_version": 2,
 "name": "SearchEnhance",
 "permissions": ["webRequest","notifications", "cookies", "tabs", "http://*/*", "https://*/*","<all_urls>" ],
 "version": "1.1.0"
}